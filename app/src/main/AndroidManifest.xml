<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hemantdave.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MoodBasedTracks"
            android:label="" />
        <activity
            android:name=".TopFiftyTracksActivity"
            android:label="@string/topFiftyTracks" />
        <activity
            android:name=".RadioHitsActivity"
            android:label="@string/radioHits" />
        <activity
            android:name=".PodcastsActivity"
            android:label="@string/podcasts" />
        <activity android:name=".BrowseMoreActivity"
            android:label="@string/browseMore"/>
    </application>

</manifest>